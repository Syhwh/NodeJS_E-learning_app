 <div class=" container">
            <div class="row">
                <div class="col-md-12">

<h2> {{instructor.first_name}}'s classes </h2>
    <table  class="table " cellspacing="0" cellpadding="0">
        <thead class="thead-dark">
            <tr>
                <th>Class Id</th>
                <th>Class Name</th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            {{#each instructor.classes}}
            <tr>
                <td>{{class_code}} </td>
                <td>{{class_title}} </td>
                <td><a href="/classes/{{class_id}}/lessons"> View Lessons</a></td>
                <td><a href="/instructors/classes/{{class_id}}/lessons/new"> Add Lessons</a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    </div>
    </div> 
    </div>