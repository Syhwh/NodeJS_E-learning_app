<div class="container">
    <div class="row">
    {{#each classes}}
    <div class="col-md-4" >
        <div class="single-item course_single_area"> 
            <div class="single-item-text">
                <h2>{{title}} </h2>
            </div>
        <div class="single-item-text-info">
            <div class="course-instructor">
        By:  <span> {{code}} </span>  </div>

        </div>
