        <p> {{description}} </p>
        <div class="button-bottom">
            <p><a class="button-default" href="/classes/{{_id}}/details" role="button">View details &raquo;</a></p>
    
        </div>
      </div>
    </div> 
    {{/each}}
  </div>
</div>

